{
  "name": "mijin",
  "homepage": "https://www.mijin.design",
  "description": "tailwindcss UI components for Vue.js",
  "version": "0.1.0",
  "author": "Leonard Lecouey <leonard.lecouey@gmail.com>",
  "main": "dist/mijin.umd.js",
  "module": "dist/mijin.umd.js",
  "unpkg": "dist/mijin.umd.js",
  "jsdelivr": "dist/mijin.umd.js",
  "license": "MIT",
  "keywords": [
    "tailwindcss",
    "vue",
    "component"
  ],
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "build": "npm run build:vue && npm run build:tailwind",
    "build:vue": "vue-cli-service build src/index.js --target lib --name mijin",
    "build:tailwind": "rimraf dist/tailwind-preset.js && rollup --config rollup.config.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@vue/cli-plugin-babel": "4.5.10",
    "@vue/cli-plugin-eslint": "4.5.10",
    "@vue/cli-plugin-unit-jest": "4.5.10",
    "@vue/cli-service": "4.5.10",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.1.2",
    "babel-eslint": "10.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-vue": "7.4.1",
    "rimraf": "2.7.1",
    "rollup": "2.36.1",
    "tailwindcss": "2.0.2",
    "vue": "2.6.12",
    "vue-template-compiler": "2.6.12"
  }
}
